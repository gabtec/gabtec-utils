{
  "name": "gabtec-utils",
  "version": "2.0.0",
  "description": "Some usefull tools",
  "type": "module",
  "exports": "./lib/index.js",
  "engines": {
    "node": ">=18.12.1"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "mocha ./tests/**/*.spec.js",
    "test-cov": "nyc --reporter=text --reporter=text-summary npm run test",
    "test-cov-web": "nyc --reporter=lcov --reporter=text-summary npm run test",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabtec/gabtec-utils.git"
  },
  "author": "Gabriel Martins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gabtec/gabtec-utils/issues"
  },
  "homepage": "https://github.com/gabtec/gabtec-utils#readme",
  "devDependencies": {
    "chai": "5.1.0",
    "mocha": "10.4.0",
    "nyc": "15.1.0",
    "sinon": "17.0.1"
  }
}
